<?xml version="1.0"?>
<ruleset name="Custom">

    <rule ref="Zend">
        <exclude name="Generic.Files.LineLength"/>
        <exclude name="Zend.NamingConventions.ValidVariableName.PrivateNoUnderscore"/>
        <exclude name="Zend.NamingConventions.ValidVariableName.NotCamelCaps">
            <exclude-pattern>*/Api/Autodata/*</exclude-pattern>
        </exclude>
        <exclude name="Zend.NamingConventions.ValidVariableName.MemberVarNotCamelCaps">
            <exclude-pattern>*/Api/OneSignal/Entity/*</exclude-pattern>
        </exclude>
    </rule>

</ruleset>